// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a;
    int* p_a;
    //pointer co ukazuje na interger
    
    a = 10;
    p_a = &a;
    //jaka je tva adresa
    
    printf("%p\n", p_a);
    //co mas v sobe ulozene
    
    printf("%i", *p_a);
    //co je na miste kam ukaujes a vispis jako intiger
    return 0;
}
